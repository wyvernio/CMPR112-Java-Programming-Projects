public class TestStuff
{
  public static void main(String [] args)
  {
    int  i;
    for (i = 1; i <= 2 ; ++ i)
    {
      printProduct();
    }
      printMessage();
  }
  public static void printMessage()
  {
    System.out.println ("HELLO WORLD ");
  }
  public static void printProduct()
  {
    int a = 4, b = 6, c;
    c = a * b;
    System.out.println ("C = " + c);
  }
}
  